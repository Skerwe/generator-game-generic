  "description": "A game development project structure generator",
  "homepage": "https://github.com/Skerwe/generator-game-generic#readme",
  "author": {
    "name": "Quintin Henn",
    "email": "quintin.henn@gmail.com",
    "url": "http://skerwe.web.za"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skerwe/generator-game-generic.git"
  },
  "keywords": [
    "yeoman-generator",
    "game-development",
    "template"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Skerwe/generator-game-generic/issues"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "yeoman-generator": "^4.4.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "acorn": "^7.1.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-xo": "^0.28.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-utils": "^2.0.0",
    "husky": "^4.0.2",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
